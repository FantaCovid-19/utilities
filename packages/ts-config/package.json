{
  "name": "@fantacovid-19/ts-config",
  "description": "Typescript configuration for Fantacovid-19 projects",
  "author": "@FantaCovid-19",
  "license": "MIT",
  "version": "0.3.1",
  "homepage": "https://github.com/FantaCovid-19/utilities/tree/main/packages/ts-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FantaCovid-19/utilities.git",
    "directory": "packages/ts-config"
  },
  "bugs": {
    "url": "https://github.com/FantaCovid-19/utilities/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "src/tsconfig.json",
  "exports": {
    ".": {
      "types": "./src/tsconfig.json",
      "import": "./src/tsconfig.json",
      "require": "./src/tsconfig.json"
    },
    "./base": {
      "types": "./src/tsconfig.json",
      "import": "./src/tsconfig.json",
      "require": "./src/tsconfig.json"
    },
    "./extra-strict": {
      "types": "./src/extraStrict.json",
      "import": "./src/extraStrict.json",
      "require": "./src/extraStrict.json"
    }
  },
  "files": [
    "src"
  ],
  "engines": {
    "node": ">=22.19.0",
    "npm": ">=11.6.0"
  },
  "scripts": {
    "test": "vitest run",
    "build": "tsc -b tests",
    "bump": "cliff-jumper",
    "check-update": "cliff-jumper --dry-run"
  },
  "dependencies": {
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@favware/cliff-jumper": "^6.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.30.0"
}

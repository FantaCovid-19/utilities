{
  "name": "@fantacovid-19/eslint-config",
  "description": "ESLint configuration for Fantacovid-19 projects",
  "author": "@fantacovid-19",
  "license": "MIT",
  "version": "0.2.0",
  "homepage": "https://github.com/fantacovid-19/utilities/tree/main/packages/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fantacovid-19/utilities.git",
    "directory": "packages/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/fantacovid-19/utilities/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "exports": {
    "import": {
      "default": "./dist/esm/index.mjs"
    },
    "require": {
      "default": "./dist/cjs/index.cjs"
    }
  },
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">=22.19.0",
    "npm": ">=11.6.0"
  },
  "scripts": {
    "test": "vitest run",
    "build": "tsup",
    "prelint": "pnpm test && pnpm build",
    "prepack": "pnpm build",
    "bump": "cliff-jumper",
    "check-update": "cliff-jumper --dry-run"
  },
  "dependencies": {
    "typescript-eslint": "^8.56.0",
    "@eslint/js": "^10.0.1",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@favware/cliff-jumper": "^6.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.30.0"
}
